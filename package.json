{
  "name": "agendator",
  "version": "1.0.0",
  "description": "Agendator backend project",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "MULTPROCESS=false DEBUG=app ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll --maxWorkers=1",
    "test:clear": "jest --clearCache",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:createOnly": "npx prisma migrate dev --create-only",
    "prisma:deploy": "npx prisma migrate deploy",
    "dependeciesUpdateCheck": "npm outdated",
    "dependeciesUpdate": "npm update"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/velloware/Agendator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/velloware/Agendator/issues"
  },
  "homepage": "https://github.com/velloware/Agendator#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prisma": "^4.3.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.2",
    "debug": "^4.3.4",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2"
  }
}
